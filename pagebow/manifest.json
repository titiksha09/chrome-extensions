{
    "name": "pageBow",
    "manifest_version": 2,
    "version": "1.0",
    "icons": {
        "128": "./images/icon.png",
        "48": "./images/icon.png",
        "16": "./images/icon.png"
    },
    "browser_action": {
        "default_icon": {
            "128": "./images/icon.png",
            "48": "./images/icon.png",
            "16": "./images/icon.png"
        },
        "default_title": "pageBow-make the page colorful"
    },
      "content_scripts": [{
          "matches": ["<all_urls>"],
          "css": ["./css/content.css"],
          "js":["./js/content.js"]
      }],
   
    "permissions": [
        "tabs"
    ]
}